<script>
	alladmin()
	async function alladmin() {
		var userid = localStorage.getItem("userid")
		if (userid != null) {
			var alladmin = await axios.get(`https://api.smartwayonline.in/distributors/`).then((res) => { return res.data }).catch((err) => { return false })
			console.log(alladmin)
			if (alladmin.length != 0) {
				alladmin.forEach((admins) => {
					document.getElementById("Regdata").innerHTML += `
            <tr class='Regdata'>
            <td>
            <div class="userDatatable-content">
                ${admins.name}
            </div>
		</td>
		<td>
            <div class="userDatatable-content">
                ${admins.email}
            </div>
		</td>
		<td>
            <div class="userDatatable-content">
                ${admins.number}
            </div>
		</td>
		<td>
            <div class="userDatatable-content">
                ${admins.password}
            </div>
		</td>
		<td>
            <div class="userDatatable-content">
				â‚¹ ${admins.wallet}
            </div>
		</td>
		<td>
            <div class="userDatatable-content">
                ${admins.location}
            </div>
		</td>
		<td>
			<div class="userDatatable-content">
                ${admins.referralcode}
            </div>
			
		</td>
		</tr>`
				})
			}
		}
		pagination(alladmin.length)
	}


	function pagination(length) {

		if (length > 10) {
			var items = $("#Regdata .Regdata");
			var numItems = length;
			console.log(numItems)
			var perPage = 10;

			items.slice(perPage).hide();

			$('#pagination-container').pagination({
				items: numItems,
				itemsOnPage: perPage,
				prevText: "&laquo;",
				nextText: "&raquo;",
				onPageClick: function (pageNumber) {
					var showFrom = perPage * (pageNumber - 1);
					var showTo = showFrom + perPage;
					items.hide().slice(showFrom, showTo).show();
				}
			});
		}
	}
	window.onload = () => {
		var userid = localStorage.getItem("userid")
		if (userid == null) {
			toastr["error"]("Please Login");
			setTimeout(() => { window.location.replace("/") }, 1000)

		}
	}
</script>

